{
  "testEnvironment": "jsdom",
  "collectCoverage": true,
  "coveragePathIgnorePatterns": [
    "/node_modules/"
  ],
  "moduleFileExtensions": [
    "js",
    "mjs"
  ],
  "transform": {
    "^.+\\.js$": "babel-jest",
    "^.+\\.jsx$": "babel-jest"
  },
  "testRegex": "((\\.|/*.)(spec|test))\\.js?$",
  "verbose": true
}
